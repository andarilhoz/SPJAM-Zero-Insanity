        if(GUI_Jogador.sanidade &gt; 0){
            GUI_Jogador.sanidade -= 1;
        }else{
            with(Jogador){
                image_blend = make_colour_rgb(220,150,150);
                vspd = -12;
                //hspd = (sign(x-other.x)*10);
                estado = hurt_state;
                move(Solo);
                
                if(instance_exists(GUI_Jogador)){
                    if(GUI_Jogador.vida &gt; 0){
                        GUI_Jogador.vida -= 1;
                    }else{
                        with(Jogador){
                            instance_destroy();
                        }
                    }
                }
            }
        }
        insano = false;
        alarm[0] = 60;
    }
}
